<Application x:Class="PasswordManagementSystem.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PasswordManagementSystem"
             StartupUri="Forms/SignIn.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary Source="/ResourceDictionary/RD_Form.xaml" x:Key="RD_Form" />
            <ResourceDictionary Source="/ResourceDictionary/RD_Main.xaml" x:Key="RD_Main" />
            <ResourceDictionary Source="/ResourceDictionary/RD_User.xaml" x:Key="RD_User" />
            <ResourceDictionary Source="/ResourceDictionary/RD_Views.xaml" x:Key="RD_Views" />
            <ResourceDictionary Source="/ResourceDictionary/RD_WindowTemplate.xaml" x:Key="RD_Template" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
